<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.test.api.mapper.MyprescriptDetailMapper">
	<insert id="insertPrescriptDetail" parameterType="com.test.api.model.PrescriptDetailDTO">
		INSERT INTO prescription_detail (prescriptionID,mediID,itemName,perDays,totalDays)
		VALUES (#{prescriptionID},#{mediID},#{itemName},#{perDays},#{totalDays})
	</insert>
	
  </mapper>
  
  